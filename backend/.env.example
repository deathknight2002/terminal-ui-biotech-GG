# Backend Configuration
NODE_ENV=development
PORT=3001

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/biotech_terminal
REDIS_URL=redis://localhost:6379

# API Keys (Optional)
PUBMED_API_KEY=
FDA_API_KEY=

# Scraper Rate Limits (requests per second or window)
# Leave empty to use defaults

# Fierce Biotech: Default 0.5 req/s (1 request per 2 seconds)
FIERCE_BIOTECH_RATE_LIMIT=0.5

# Science Daily: Default 1 req/s
SCIENCE_DAILY_RATE_LIMIT=1

# BioSpace: Default 1 req/s
BIOSPACE_RATE_LIMIT=1

# BioPharm Dive: Default 1 req/s
BIOPHARMDIVE_RATE_LIMIT=1

# Endpoints News: Default 1 req/s
ENDPOINTS_NEWS_RATE_LIMIT=1

# Cache Configuration
# Cache TTL in milliseconds (default 14400000 = 4 hours)
SCRAPER_CACHE_TTL=14400000

# Circuit Breaker Configuration
CIRCUIT_BREAKER_FAILURE_THRESHOLD=5
CIRCUIT_BREAKER_RESET_TIMEOUT=60000

# Worker Pool Configuration
WORKER_POOL_MAX_WORKERS=
WORKER_POOL_TASK_TIMEOUT=30000

# Logging
LOG_LEVEL=info

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# JWT Secret
JWT_SECRET=your-secret-key-here

# Yahoo Finance (for market data)
YAHOO_FINANCE_ENABLED=true

# OpenBB Integration
OPENBB_ENABLED=true
